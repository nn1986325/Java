import java.util.*;
public class Factor
{
public static void main(String[] args)
{
int num;
Scanner sc = new Scanner(System.in);
System.out.print("Enter a number : ");
num = sc.nextInt();
int i, count = 0;
for(i = 1; i <= num; i++)
{
if(num % i == 0)
{
count = count + 1;
}
}
System.out.print("NUmber of factors: " + count);
}
}
